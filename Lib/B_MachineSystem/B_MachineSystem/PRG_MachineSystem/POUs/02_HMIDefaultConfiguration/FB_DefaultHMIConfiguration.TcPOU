itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 6, iMode := 3, itfModule := PRG_ControlHMI.fbPrepareDataModul_06_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 6, iMode := 4, itfModule := PRG_ControlHMI.fbPrepareDataModul_06_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 6, iMode := 5, itfModule := PRG_ControlHMI.fbPrepareDataModul_06_XX);

itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 7, iMode := 1, itfModule := PRG_ControlHMI.fbPrepareDataModul_07_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 7, iMode := 2, itfModule := PRG_ControlHMI.fbPrepareDataModul_07_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 7, iMode := 3, itfModule := PRG_ControlHMI.fbPrepareDataModul_07_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 7, iMode := 4, itfModule := PRG_ControlHMI.fbPrepareDataModul_07_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 7, iMode := 5, itfModule := PRG_ControlHMI.fbPrepareDataModul_07_XX);
]]></ST>
      </Implementation>
    </Method>
