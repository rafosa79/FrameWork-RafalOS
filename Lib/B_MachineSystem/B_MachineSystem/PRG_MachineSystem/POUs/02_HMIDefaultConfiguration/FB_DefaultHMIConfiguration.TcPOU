THIS^.SetText (eLocation := E_BaseLocation.pl, wsDescription := "Receptury");
THIS^.SetText (eLocation := E_BaseLocation.en, wsDescription := "Recipe");
THIS^.SetText (eLocation := E_BaseLocation.de, wsDescription := "Rezept");
