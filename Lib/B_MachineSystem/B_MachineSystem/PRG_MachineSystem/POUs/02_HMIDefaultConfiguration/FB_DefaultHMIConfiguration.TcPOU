itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 5, iMode := 4, itfModule := PRG_ControlHMI.fbPrepareDataModul_05_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 5, iMode := 5, itfModule := PRG_ControlHMI.fbPrepareDataModul_05_XX);

itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 6, iMode := 1, itfModule := PRG_ControlHMI.fbPrepareDataModul_06_XX);
