    <Method Name="AddPrepareScreenModule" Id="{63b11d9d-8cb5-0f9d-017d-ab57b2966545}">
      <Declaration><![CDATA[(*			
	RafalOS
	Rafał Osajda		19.11.2025		TC 4024.66
	Dodanie modułów do przygotowywania stron do wyświetlenia
*)
METHOD AddPrepareScreenModule : BOOL
VAR_INPUT
	itfHMIManager	: B_HMI_Manager.ITF_HMIManagerConfigure;
END_VAR
VAR
	iforScreen	: INT;
	iforMode	: INT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[	(*	Dodanie modułów do machine controil	*)
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_Empty);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_00_01);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_00_02);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_00_03);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_00_04);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_00_05);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_01_XX);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_02_XX);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_03_XX);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_04_XX);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_05_XX);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_06_XX);
itfHMIManager.AddPrepareScreenModuleFromConfiguration (itfModule := PRG_ControlHMI.fbPrepareDataModul_07_XX);


	(*	Przypisanie do wszystkich pól pustego FB *)
FOR iforScreen := 0 TO GVL_HMIEConstans.MAX_NUMBER_OF_BUTTON_IN_MENU DO 
	FOR iforMode := 0 TO GVL_SaMConstans.NUMBER_OF_MODES DO 
		itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (
			iScreen 	:= iforScreen, 
			iMode 		:= iforMode, 
			itfModule 	:= PRG_ControlHMI.fbPrepareDataModul_Empty
		);
	END_FOR
END_FOR
	(*	Przypisanie modułów do aktywnych akien 	*)
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 0, iMode := 1, itfModule := PRG_ControlHMI.fbPrepareDataModul_00_01);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 0, iMode := 2, itfModule := PRG_ControlHMI.fbPrepareDataModul_00_02);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 0, iMode := 3, itfModule := PRG_ControlHMI.fbPrepareDataModul_00_03);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 0, iMode := 4, itfModule := PRG_ControlHMI.fbPrepareDataModul_00_04);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 0, iMode := 5, itfModule := PRG_ControlHMI.fbPrepareDataModul_00_05);

itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 1, iMode := 1, itfModule := PRG_ControlHMI.fbPrepareDataModul_01_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 1, iMode := 2, itfModule := PRG_ControlHMI.fbPrepareDataModul_01_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 1, iMode := 3, itfModule := PRG_ControlHMI.fbPrepareDataModul_01_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 1, iMode := 4, itfModule := PRG_ControlHMI.fbPrepareDataModul_01_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 1, iMode := 5, itfModule := PRG_ControlHMI.fbPrepareDataModul_01_XX);

itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 2, iMode := 1, itfModule := PRG_ControlHMI.fbPrepareDataModul_02_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 2, iMode := 2, itfModule := PRG_ControlHMI.fbPrepareDataModul_02_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 2, iMode := 3, itfModule := PRG_ControlHMI.fbPrepareDataModul_02_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 2, iMode := 4, itfModule := PRG_ControlHMI.fbPrepareDataModul_02_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 2, iMode := 5, itfModule := PRG_ControlHMI.fbPrepareDataModul_02_XX);

itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 3, iMode := 1, itfModule := PRG_ControlHMI.fbPrepareDataModul_03_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 3, iMode := 2, itfModule := PRG_ControlHMI.fbPrepareDataModul_03_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 3, iMode := 3, itfModule := PRG_ControlHMI.fbPrepareDataModul_03_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 3, iMode := 4, itfModule := PRG_ControlHMI.fbPrepareDataModul_03_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 3, iMode := 5, itfModule := PRG_ControlHMI.fbPrepareDataModul_03_XX);

itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 4, iMode := 1, itfModule := PRG_ControlHMI.fbPrepareDataModul_04_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 4, iMode := 2, itfModule := PRG_ControlHMI.fbPrepareDataModul_04_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 4, iMode := 3, itfModule := PRG_ControlHMI.fbPrepareDataModul_04_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 4, iMode := 4, itfModule := PRG_ControlHMI.fbPrepareDataModul_04_XX);
itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 4, iMode := 5, itfModule := PRG_ControlHMI.fbPrepareDataModul_04_XX);

itfHMIManager.LinkPrepareScreenToArrayFromConfiguration (iScreen := 5, iMode := 1, itfModule := PRG_ControlHMI.fbPrepareDataModul_05_XX);
