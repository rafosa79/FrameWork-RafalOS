THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB - 3");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB - 3");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB - 3");
THIS^.itfComponent3.SetName (stName	:= THIS^.GetTexts ());]]></ST>
      </Implementation>
