THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa control elementu PB - 2");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa control elementu PB - 2");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa control elementu PB - 2");
THIS^.itfControlElement2.SetName (stName	:= THIS^.GetTexts ());

THIS^.itfGroupElement1				:= itfGroupElement1;
THIS^.AddControlElement (itfControlElement	:= itfGroupElement1);
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa group elementu PB - 1");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa group elementu PB - 1");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa group elementu PB - 1");
THIS^.itfGroupElement1.SetName (stName	:= THIS^.GetTexts ());

THIS^.itfComponent1					:= itfComponent1;
THIS^.AddComponent (itfComponent	:= itfComponent1);
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB - 1");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB - 1");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB - 1");
THIS^.itfComponent1.SetName (stName	:= THIS^.GetTexts ());

THIS^.itfComponent2					:= itfComponent2;
THIS^.AddComponent (itfComponent	:= itfComponent2);
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB -2");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB -2");
THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa komponentu PB -2");
THIS^.itfComponent2.SetName (stName	:= THIS^.GetTexts ());

THIS^.itfComponent3					:= itfComponent3;
