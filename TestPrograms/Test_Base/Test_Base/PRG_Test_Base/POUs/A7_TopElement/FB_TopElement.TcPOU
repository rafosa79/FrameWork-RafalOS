THIS^.SetText (eLocation 			:= E_BaseLocation.pl, wsDescription := "Nazwa control eleemntu PB - 1");
THIS^.itfControlElement1.SetName (stName	:= THIS^.GetTexts ());

THIS^.itfControlElement2			:= itfControlElement2;
