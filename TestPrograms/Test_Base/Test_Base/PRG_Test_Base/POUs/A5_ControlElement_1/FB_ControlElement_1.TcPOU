    <Declaration><![CDATA[FUNCTION_BLOCK FB_ControlElement_1 EXTENDS FB_BaseControlElement IMPLEMENTS ITF_ControlElement_1, ITF_ControlElement_HMI_Init
VAR
	itfComponent1				: ITF_Component;
	itfComponent2				: ITF_Component;
	
	bHomingFinished				: BOOL;
END_VAR
]]></Declaration>
    <Implementation>
