    <Method Name="AddTopElementToMachineControl" Id="{14c2a0df-b97e-48ab-8674-34192113ffc6}">
      <Declaration><![CDATA[(*			
	EVERDE sp. z o.o.
	Rafał Osajda		29.10.2025		TC 4024.66
	Dodawanie TopElementu do systemu
*)
METHOD AddTopElementToMachineControl : BOOL
VAR_INPUT
	itfMachineControl	: B_MachineControl.ITF_MachineControlConfigure;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[THIS^.SetText (eLocation := E_BaseLocation.pl, wsDescription := "TopElement 1 pl");
THIS^.SetText (eLocation := E_BaseLocation.en, wsDescription := "TopElement 1 en");
THIS^.SetText (eLocation := E_BaseLocation.de, wsDescription := "TopElement 1 de");
GVL_TopElement1.fbTopElement1.SetName (stName := THIS^.GetTexts ());
itfMachineControl.AddTopElementFromConfiguration (fbTopElement	:= GVL_TopElement1.fbTopElement1);

THIS^.SetText (eLocation := E_BaseLocation.pl, wsDescription := "TopElement 2 pl");
THIS^.SetText (eLocation := E_BaseLocation.en, wsDescription := "TopElement 2 en");
THIS^.SetText (eLocation := E_BaseLocation.de, wsDescription := "TopElement 2 de");
GVL_TopElement1.fbTopElement1.SetName (stName := THIS^.GetTexts ());
itfMachineControl.AddTopElementFromConfiguration (fbTopElement	:= GVL_TopElement2.fbTopElement2);]]></ST>
      </Implementation>
