      </Implementation>
    </Method>
    <Method Name="SetVisibleButton" Id="{9b32dc56-5171-0eb1-09b1-fe51305e187e}" FolderPath="ITF_HMIEMenu\">
      <Declaration><![CDATA[METHOD SetVisibleButton : BOOL
VAR_INPUT
