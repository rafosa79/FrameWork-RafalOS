      <Declaration><![CDATA[METHOD PUBLIC IsActive : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IsActive			:= THIS^.bActive;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="IsEnabled" Id="{cda05f82-8147-0995-0f93-35b325b598b2}">
      <Declaration><![CDATA[(*			
	EVERDE sp. z o.o.
	Rafał Osajda		03.11.2025		TC 4024.66
	Zwraca czy element jest właczony w systemie
*)
METHOD PUBLIC IsEnabled : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IsEnabled			:= THIS^.bIsEnabled;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="ResetTime" Id="{99320a18-e3c5-086e-3eea-75bf7272336f}">
      <Declaration><![CDATA[METHOD PUBLIC ResetTime
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
