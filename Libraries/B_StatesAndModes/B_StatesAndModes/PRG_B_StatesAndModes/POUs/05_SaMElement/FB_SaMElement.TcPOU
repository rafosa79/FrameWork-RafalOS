  <POU Name="FB_SaMElement" Id="{03aaa14d-d8d1-0ccd-22ca-7daa896b0e09}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK ABSTRACT FB_SaMElement EXTENDS FB_BaseComponent IMPLEMENTS ITF_SaMElement
VAR
	bActive						: BOOL;				//informacja czy dany element jest aktywny 
	bIsEnabled					: BOOL;				//informacje czy dany element jest włączony
//	refstDaneSITP				: REFERENCE TO ST_SITP_DaneElementu;
END_VAR
VAR PERSISTENT
	ltWorkTime					: LTIME;				//czas pracy w danym stanie - trybie
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Method Name="IncrementTime" Id="{304bcff5-f12b-0ac9-38cb-f597f33e927e}">
      <Declaration><![CDATA[METHOD PUBLIC IncrementTime
VAR_INPUT
	tInterval				: TIME := T#100MS;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[THIS^.ltWorkTime			:= THIS^.ltWorkTime + TO_LTIME (tInterval);
]]></ST>
      </Implementation>
    </Method>
