    <Method Name="AddPrepareScreenModuleFromManager" Id="{3a4d88d9-e1f1-069f-33f6-e25e0914935a}" FolderPath="ITF_MachineControl_HMI\">
      <Declaration><![CDATA[METHOD PUBLIC AddPrepareScreenModuleFromManager : BOOL
VAR_INPUT
	itfModule	: ITF_MCHMIPrepareScreen_Init;// ITF_MCHMIPrepareDataForScreen;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF (THIS^.iNumberOfAddedPrepScren + 1 <= 20) THEN
	THIS^.AddComponent (itfComponentHMI := itfModule);
	THIS^.iNumberOfAddedPrepScren := THIS^.iNumberOfAddedPrepScren + 1;
	THIS^.arrPrepareScreen [THIS^.iNumberOfAddedPrepScren] := itfModule;
	AddPrepareScreenModuleFromManager := TRUE;
ELSE
	AddPrepareScreenModuleFromManager := FALSE;
END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="ClearAfterScreenFroHMIManager" Id="{ba105132-73c6-04e7-2948-825f2b608f69}" FolderPath="ITF_MachineControl_HMI\">
      <Declaration><![CDATA[(*			
	RafalOS
	Rafał Osajda		19.11.2025		TC 4024.66
	Sprzątanie po stronie wywoływane z HMIManager i wywołująca odpowiednią metode z ITF z arrPrepareScreen
*)
METHOD ClearAfterScreenFroHMIManager : BOOL
VAR_INPUT
	iScreen		: INT;
	iMode		: INT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF (THIS^.arrPrepareScreen_Execute [iScreen][iMode] <> 0) THEN
	THIS^.arrPrepareScreen_Execute [iScreen][iMode].ClearAfterScreen ();
END_IF
]]></ST>
      </Implementation>
    </Method>
    <Method Name="FB_init" Id="{6e789ec0-7c6a-0ef7-1933-c74097114aeb}">
      <Declaration><![CDATA[METHOD FB_init : BOOL
VAR_INPUT
	bInitRetains : BOOL; // if TRUE, the retain variables are initialized (warm start / cold start)
	bInCopyCode : BOOL;  // if TRUE, the instance afterwards gets moved into the copy code (online change)
	itfBaseLoggerConnector				: ITF_BaseLogger_HMI_Connector;	//bezpośredni łącznik do logera
	itfMachineControlConnector			: ITF_MCMachineControl_HMI_Connector;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[//Przypisanie do elementu który własnie stworzyliśmy
THIS^.itfMachineControlConnector := itfMachineControlConnector;
//Przypisanie do elementów z biblioteki
THIS^.itfBaseLoggerElementConnector := itfMachineControlConnector;
THIS^.itfBaseComponentConnector := itfMachineControlConnector;
THIS^.itfBaseControlElementConnector := itfMachineControlConnector;
THIS^.itfBaseGroupElementConnector := itfMachineControlConnector;
THIS^.itfBaseTopElementConnector := itfMachineControlConnector;
//
THIS^.itfBaseLoggerConnector := itfBaseLoggerConnector;

IF (THIS^.itfMachineControlConnector <> 0 AND
	THIS^.itfBaseLoggerElementConnector <> 0 AND
	THIS^.itfBaseComponentConnector	<> 0 AND
	THIS^.itfBaseControlElementConnector <> 0 AND
	THIS^.itfBaseGroupElementConnector <> 0 AND
	THIS^.itfBaseTopElementConnector <> 0 AND
	THIS^.itfBaseLoggerConnector <> 0) THEN
	THIS^.bIsInit						:= TRUE;
END_IF]]></ST>
      </Implementation>
    </Method>
