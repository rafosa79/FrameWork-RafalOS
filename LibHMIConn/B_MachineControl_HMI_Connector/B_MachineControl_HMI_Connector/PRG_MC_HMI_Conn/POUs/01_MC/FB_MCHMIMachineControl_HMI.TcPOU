THIS^.refstHMICommonData REF= stData.stCommonData;
THIS^.arrPrepareScreen [2].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [3].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [4].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [5].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [6].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [7].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [8].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [9].LinkHMICommonData (stHMICommonData := stData.stCommonData);
