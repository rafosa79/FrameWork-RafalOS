    <Method Name="GetInformationLatestRecipes" Id="{0ee08023-6867-0e9d-3958-b8c640468601}" FolderPath="ITF_MachineControl_HMI\">
      <Declaration><![CDATA[(*			
	RafalOS
	Rafał Osajda		07.12.2025		TC 4024.66
	Pobierz informacje o aktualnym stanie recepur tzn o tym czy jest wybrana a jak tak to która
*)
METHOD PUBLIC GetInformationLatestRecipes
VAR_INST
	tonChangeSelectRecipe	: TON;
	rtNewRecipeSelected		: R_TRIG;
	ftNewRecipeSelected		: F_TRIG;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[THIS^.refstHMICommonData.stRecipeInformation.bRecipeSelected := THIS^.itfRecipe.IsRecipeSelected ();
rtNewRecipeSelected (CLK := THIS^.refstHMICommonData.stRecipeInformation.bRecipeSelected);
ftNewRecipeSelected (CLK := THIS^.refstHMICommonData.stRecipeInformation.bRecipeSelected);
tonChangeSelectRecipe (PT := T#3S);

IF (rtNewRecipeSelected.Q OR 
	THIS^.refstHMICommonData.stRecipeInformation.bNewSelectedRecipe) THEN
	;//jest wybrana
	THIS^.refstHMICommonData.stRecipeInformation.bNewSelectedRecipe := FALSE;
	tonChangeSelectRecipe.IN := TRUE;
	THIS^.refstHMICommonData.stRecipeInformation.dwBackgroud := 16#FF00FF00;//zielony
	THIS^.refstHMICommonData.stRecipeInformation.dwText := 16#FF000000;//cxzarny
	THIS^.refstHMICommonData.stRecipeInformation.wsRecipeInformation := THIS^.GetTextFromLabelForActiveLocation (stLabel := THIS^.stRecipeInfoSelect);
END_IF
IF (ftNewRecipeSelected.Q) THEN
	THIS^.refstHMICommonData.stRecipeInformation.dwBackgroud := 16#FFFF0000; //czerwony
	THIS^.refstHMICommonData.stRecipeInformation.dwText := 16#FFFFFF00;//zółty
	THIS^.refstHMICommonData.stRecipeInformation.wsRecipeInformation := THIS^.GetTextFromLabelForActiveLocation (stLabel := THIS^.stRecipeInfoNotSelect);
END_IF
IF (tonChangeSelectRecipe.Q) THEN 
	tonChangeSelectRecipe.IN := FALSE;
	THIS^.refstHMICommonData.stRecipeInformation.bNewSelectedRecipe := FALSE;
	THIS^.refstHMICommonData.stRecipeInformation.dwBackgroud := 16#FF00FF00;//zielony
	THIS^.refstHMICommonData.stRecipeInformation.dwText := 16#FF000000;//cxzarny
	THIS^.refstHMICommonData.stRecipeInformation.wsRecipeInformation := THIS^.GetNumberAndNameRecipe ();
END_IF
]]></ST>
      </Implementation>
    </Method>
