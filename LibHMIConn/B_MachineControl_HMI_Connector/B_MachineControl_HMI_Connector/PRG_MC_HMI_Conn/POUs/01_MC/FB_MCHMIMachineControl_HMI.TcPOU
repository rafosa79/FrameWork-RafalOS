]]></ST>
      </Implementation>
    </Method>
    <Method Name="LinkMachineControlData" Id="{7164a5f6-e4e6-0149-033c-51ba8887454c}" FolderPath="ITF_MachineControl_HMI\">
      <Declaration><![CDATA[METHOD PUBLIC LinkMachineControlData : BOOL
VAR_INPUT
	stData			: REFERENCE TO ST_MCHMIMachineControl_HMI_Link;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[THIS^.refstMachineControlData REF= stData.stMachineControl;
//w tablicy pole 1 zajmuje metoda pusta domyślna do niej się nie linkuje danych
THIS^.arrPrepareScreen [2].LinkDataHMI_00_01 (refstData := stData.stPrepScr_00_01);
THIS^.arrPrepareScreen [3].LinkDataHMI_00_02 (refstData := stData.stPrepScr_00_02);
THIS^.arrPrepareScreen [4].LinkDataHMI_00_03 (refstData := stData.stPrepScr_00_03);
THIS^.arrPrepareScreen [5].LinkDataHMI_00_04 (refstData := stData.stPrepScr_00_04);
THIS^.arrPrepareScreen [6].LinkDataHMI_00_05 (refstData := stData.stPrepScr_00_05);

THIS^.arrPrepareScreen [7].LinkDataHMI_01_XX (refstData := stData.stPrepScr_01_XX);
THIS^.arrPrepareScreen [8].LinkDataHMI_02_XX (refstData := stData.stPrepScr_02_XX);
THIS^.arrPrepareScreen [9].LinkDataHMI_03_XX (refstData := stData.stPrepScr_03_XX);
THIS^.arrPrepareScreen [10].LinkDataHMI_04_XX (refstData := stData.stPrepScr_04_XX);
THIS^.arrPrepareScreen [11].LinkDataHMI_05_XX (refstData := stData.stPrepScr_05_XX);
THIS^.arrPrepareScreen [12].LinkDataHMI_06_XX (refstData := stData.stPrepScr_06_XX);
THIS^.arrPrepareScreen [13].LinkDataHMI_07_XX (refstData := stData.stPrepScr_07_XX);

THIS^.arrPrepareScreen [2].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [3].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [4].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [5].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [6].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [7].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [8].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [9].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [10].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [11].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [12].LinkHMICommonData (stHMICommonData := stData.stCommonData);
THIS^.arrPrepareScreen [13].LinkHMICommonData (stHMICommonData := stData.stCommonData);

THIS^.refstBaseLoggerElementDataHMI REF= stData.stBaseLoggerElement;
THIS^.refstBaseComponentDataHMI REF= stData.stBaseComponent;
THIS^.refstBaseControlElementDataHMI REF= stData.stBaseControlElement;
THIS^.refstBaseGroupElementDataHMI REF= stData.stBaseGroupElement;
THIS^.refstBaseTopElementDataHMI REF= stData.stBaseTopElement;

THIS^.CreateLinkToEventsMemory ();

IF (__ISVALIDREF (THIS^.refstMachineControlData) AND THIS^.IsLinkedReferences ()) THEN
	THIS^.bIsValidData := TRUE;
	LinkMachineControlData := TRUE;
END_IF
]]></ST>
      </Implementation>
    </Method>
