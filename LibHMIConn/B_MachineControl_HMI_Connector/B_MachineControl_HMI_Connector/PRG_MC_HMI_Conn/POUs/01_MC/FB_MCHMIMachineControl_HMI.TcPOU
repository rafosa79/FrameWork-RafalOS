THIS^.CreateLinkToEventsMemory ();

IF (__ISVALIDREF (THIS^.refstMachineControlData) AND THIS^.IsLinkedReferences ()) THEN
	THIS^.bIsValidData := TRUE;
	LinkMachineControlData := TRUE;
END_IF
]]></ST>
